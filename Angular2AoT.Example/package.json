    "@angular/http": "*",
    "@angular/platform-browser": "*",
    "@angular/platform-browser-dynamic": "*",
    "@angular/platform-server": "*",
    "@angular/router": "*",
    "@angular/tsc-wrapped": "*",
    "@angular/upgrade": "*",
    "angular-in-memory-web-api": "*",
    "core-js": "*",
    "rxjs": "*",
    "systemjs": "*",
    "zone.js": "*"
  },
  "devDependencies": {
    "@types/angular": "*",
    "@types/angular-animate": "*",
