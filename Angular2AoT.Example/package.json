    "@types/angular-route": "*",
    "@types/angular-sanitize": "*",
    "@types/jasmine": "*",
    "@types/node": "*",
    "babel-cli": "*",
    "babel-preset-angular2": "*",
    "babel-preset-es2015": "*",
    "canonical-path": "*",
    "concurrently": "*",
